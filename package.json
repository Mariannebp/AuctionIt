{
  "name": "auctionit",
  "version": "0.0.2",
  "description": "Semester Project 2",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css",
    "test": "npm test",
    "format": "prettier -w src/**/*.mjs",
    "lint": "eslint src/**/*.mjs",
    "lint-fix": "eslint src/**/*.mjs --cache --fix",
    "prepare": "husky install",
    "vite-dev": "vite",
    "vite-build": "vite-build",
    "vite-preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mariannebp/AuctionIt.git"
  },
  "author": "Marianne Pedersen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mariannebp/AuctionIt/issues"
  },
  "homepage": "https://github.com/Mariannebp/AuctionIt#readme",
  "devDependencies": {
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "vite": "^3.2.7"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "word-wrap": "^1.2.5"
  },
  "lint-staged": {
    "*.mjs": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
